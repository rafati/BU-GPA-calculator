# AI Provider Configuration
# Select which AI provider to use - options: 'gemini', 'huggingface', 'openai', 'none'
AI_PROVIDER=gemini

# Enable AI features (set to 'true' to enable)
ENABLE_AI=true
# Google Gemini API Key
# Get this from https://ai.google.dev/
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro

# AI Generation Parameters
AI_MAX_TOKENS=500
AI_TEMPERATURE=0.7
AI_TOP_P=0.9
